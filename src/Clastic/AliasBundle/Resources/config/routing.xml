<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="clastic_backoffice_alias_list" path="/alias/list">
        <default key="_controller">ClasticAliasBundle:Alias:list</default>
    </route>

    <route id="clastic_backoffice_alias_form" path="/alias/edit/{id}">
        <default key="_controller">ClasticAliasBundle:Alias:form</default>
        <default key="id">null</default>
    </route>
</routes>