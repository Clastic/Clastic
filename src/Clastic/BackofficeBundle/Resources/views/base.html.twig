    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>
<div class="sticky-wrapper">
    <nav class="navbar navbar-default" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="{{ path('clastic_backoffice_dashboard') }}">Clastic</a>
            </div>
            <div id="navbar" class="navbar-collapse collapse">
                <ul class="nav navbar-nav navbar-right">
                    <li class="dropdown modules js-nav-module-list">
                        <a aria-expanded="true" role="button" data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <i class="fa fa-dot-circle-o"></i> Modules
                            <span class="caret"></span>
                        </a>
                        <ul role="menu" class="dropdown-menu modules">
                            <li class="dropdown-header">Modules</li>
                            <li>
                                <ul>
                                    <li><a href="{{ path('clastic_backoffice_list') }}">Alias</a></li>
                                    <li><a href="{{ path('clastic_backoffice_list') }}">Text</a></li>
                                </ul>
                            </li>
                            <li class="divider"></li>
                            <li class="dropdown-header">Administration</li>
                            <li>
                                <ul>
                                    <li><a href="{{ path('clastic_backoffice_list') }}">Users</a></li>
                                    <li><a href="{{ path('clastic_backoffice_list') }}">Settings</a></li>
                                </ul>
                            </li>
                            {#<li><a href="#"><i class="fa fa-gear"></i> Settings</a></li>#}
                        </ul>
                    </li>
                    <li class="dropdown">
                        <a aria-expanded="true" role="button" data-toggle="dropdown" class="dropdown-toggle" href="#">
                            <img src="{{ gravatar('dries@nousefreak.be') }}" class="img-circle" alt=""/>
                            &nbsp;
                            <span class="caret"></span>
                        </a>
                        <ul role="menu" class="dropdown-menu">
                            <li><a href="#"><i class="fa fa-user"></i> Profile</a></li>
                            <li><a href="#"><i class="fa fa-gear"></i> Settings</a></li>
                            <li class="divider"></li>
                            <li><a href="#"><i class="fa fa-sign-out"></i> Logout</a></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

    <nav class="breadcrumb-container">
        <div class="container">
            <ol class="breadcrumb">
                <li><a href="{{ path('clastic_backoffice_dashboard') }}">Home</a></li>
                <li><a href="#">Library</a></li>
                <li class="active">Data</li>
            </ol>
        </div>
    </nav>

    <div class="main-container container">
        <header class="container-header">
            <h1>{% block page_title %}{% endblock %}</h1>
            <div class="header-actions">
                {% block header_actions %}{% endblock %}
            </div>
        </header>
        {% block content %}{% endblock %}
    </div>
    <div class="push"></div>
</div>
<footer class="footer">
    <div class="container">
        <div class="row">
            <div class="col-md-6"></div>
            <div class="col-md-6"><span class="pull-right">&copy; Dries De Peuter</span></div>
        </div>
    </div>
</footer>

