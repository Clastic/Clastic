<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="clastic_backoffice_block_list" path="/block/list">
        <default key="_controller">ClasticBlockBundle:Block:list</default>
    </route>

    <route id="clastic_backoffice_block_form" path="/block/edit/{id}">
        <default key="_controller">ClasticBlockBundle:Block:form</default>
        <default key="id" xsi:nil="true" />
    </route>
</routes>
